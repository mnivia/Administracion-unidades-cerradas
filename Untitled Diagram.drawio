        </mxCell>
        <mxCell id="GmmOpCF_TdwQESO5ywc--6" value="- Se debe notificar a qué hora y cuál empresa realizó el envío&amp;nbsp;&lt;br&gt;&lt;br&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;labelPosition=center;verticalLabelPosition=middle;verticalAlign=top;labelBackgroundColor=none;labelBorderColor=none;" vertex="1" parent="1">
