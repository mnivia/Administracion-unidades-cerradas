        <mxCell id="svupegZ1BK7ZTZ6G6fqw-3" value="1.Tener reglamento interno" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
